{
    "name": "custom-theme/wordpress",
    "description": "Custom WordPress theme with Timber and ACF",
    "type": "wordpress-theme",
    "require": {
        "timber/timber": "^2.0",
        "composer/installers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "CustomTheme\\": "inc/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
