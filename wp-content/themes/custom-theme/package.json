{
    "name": "custom-theme",
    "version": "1.0.0",
    "description": "Custom WordPress theme with Timber, ACF Pro, and modern build tools",
    "scripts": {
        "dev": "webpack --mode development --watch",
        "build": "webpack --mode production",
        "scss:watch": "sass --watch assets/scss:assets/css",
        "scss:build": "sass assets/scss:assets/css --style compressed",
        "start": "npm run dev"
    },
    "devDependencies": {
        "@babel/core": "^7.22.0",
        "@babel/preset-env": "^7.22.0",
        "@babel/preset-react": "^7.27.1",
        "babel-loader": "^9.1.0",
        "css-loader": "^6.8.0",
        "mini-css-extract-plugin": "^2.7.0",
        "sass": "^1.63.0",
        "sass-loader": "^13.3.0",
        "webpack": "^5.88.0",
        "webpack-cli": "^5.1.0"
    },
    "dependencies": {
        "@wordpress/block-editor": "^12.5.0",
        "@wordpress/blocks": "^12.4.0",
        "@wordpress/components": "^25.3.0",
        "@wordpress/element": "^5.4.0",
        "@wordpress/i18n": "^4.27.0"
    }
}
